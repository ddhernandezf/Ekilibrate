        </div>
    </div>

    <div class="col-lg-4 col-sm-4 col-xs-12">
        <div class="widget">
            <div class="widget-header bg-themethirdcolor">
                <span class="widget-caption">Tiempos de comida</span>
            </div>
            <div class="widget-body">
                <div id="internal-row" class="internal-row">
                    <div class="grid-tcomida">
                        <div class="grid-row">
                            @if (Convert.ToBoolean(Model.Desayuno))
                            {
                                <div class="grid-cell" style="background-color: #E4E2E5;">D</div>
                            }
                            else
                            {
                                <div class="grid-cell">D</div>
                            }

                            @if (Convert.ToBoolean(Model.RefaAM))
                            {
                                <div class="grid-cell" style="background-color: #E4E2E5;">R</div>
                            }
                            else
                            {
                                <div class="grid-cell">R</div>
                            }

                            @if (Convert.ToBoolean(Model.Almuerzo))
                            {
                                <div class="grid-cell" style="background-color: #E4E2E5;">A</div>
                            }
                            else
                            {
                                <div class="grid-cell">A</div>
                            }

                            @if (Convert.ToBoolean(Model.RefaPM))
                            {
                                <div class="grid-cell" style="background-color: #E4E2E5;">R</div>
                            }
                            else
                            {
                                <div class="grid-cell">R</div>
                            }

                            @if (Convert.ToBoolean(Model.Cena))
                            {
                                <div class="grid-cell" style="background-color: #E4E2E5;">C</div>
                            }
                            else
                            {
                                <div class="grid-cell">C</div>
                            }
                            <div class="grid-cell">CN</div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="col-lg-4 col-sm-4 col-xs-12">
        <div class="widget">
            <div class="widget-header bg-themethirdcolor">
                <span class="widget-caption">Evaluación personal</span>
            </div>
            <div class="widget-body">
                <div id="internal-row" class="internal-row">
                    @{
                        int flagevpe1 = 3;
